{
  "name": "newspaper-admin",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --no-cache --max-warnings=3",
      "prettier --write"
    ],
    "*.module.css": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "env-cmd -f environments/dev.env next dev",
    "build:dev": "env-cmd -f environments/dev.env next build",
    "build:staging": "env-cmd -f environments/test.env next build",
    "build:prod": "env-cmd -f environments/prod.env next build",
    "dev:prod": "env-cmd -f environments/prod.env next dev",
    "start": "next start",
    "lint": "next lint --no-cache --max-warnings=5",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "env-cmd": "^10.1.0",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
